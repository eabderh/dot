# localhost:6000

server {
	listen 80;
	server_name pyserve.bananalias.xyz;
	set $remote_fulladd "${remote_addr}:${remote_port}";
	location / {
		proxy_pass http://localhost:6000;
		proxy_http_version 1.1;
		proxy_set_header Host $http_host;
		proxy_set_header X-Real-IP $remote_addr;
		proxy_redirect off;
	}
}
